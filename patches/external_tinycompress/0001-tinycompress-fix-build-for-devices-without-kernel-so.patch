diff --git a/Android.mk b/Android.mk
index bdb58b6..7f00858 100644
--- a/Android.mk
+++ b/Android.mk
@@ -2,8 +2,13 @@ LOCAL_PATH:= $(call my-dir)
 
 include $(CLEAR_VARS)
 LOCAL_C_INCLUDES:= $(LOCAL_PATH)/include
+
+# fix for devices without kernel sources (ugh!)
+ifeq ($(TARGET_PREBUILT_KERNEL),)
